    "type": "git",
    "url": "https://github.com/Microsoft/monaco-languages"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/monaco-languages/issues"
  },
  "devDependencies": {
    "jsdom-no-contextify": "^3.1.0",
    "mocha": "^5.2.0",
    "monaco-editor-core": "0.13.0",
    "monaco-plugin-helpers": "^1.0.2",
    "requirejs": "^2.3.5",
    "typescript": "2.7.2",
    "uglify-js": "^3.3.14"
  }
}
