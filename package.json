{
  "name": "monaco-languages",
  "version": "1.3.0",
  "description": "Bundle of many languages for the Monaco Editor.",
  "scripts": {
    "compile": "mrmdir ./release && tsc -p ./src/tsconfig.json && tsc -p ./src/tsconfig.esm.json",
